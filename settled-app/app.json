{
  "expo": {
    "android": {
      "package": "com.settled.app",
       "googleServicesFile": "./google-services.json"
    },
    "name": "Pendent",
    "slug": "settled-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pendent-logo-stacked-square.png",
    "splash": {
      "image": "./assets/images/pendent-logo-stacked-square.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
